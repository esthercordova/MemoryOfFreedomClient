<h4>{{title}}</h4>
{{#each answer}}
<ul>
  <li>{{this}}</li>
</ul>
{{/each}}

<button id="right" class="btn btn-info answerButton">Got it right!</button>
<button id="wrong" class="btn btn-info answerButton">Next time</button>
